#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'flawless==0.3.1','console_scripts','flawless'
__requires__ = 'flawless==0.3.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('flawless==0.3.1', 'console_scripts', 'flawless')()
    )
