<table class="form-table" i18n:domain="waeup.kofa">
  <tbody>
    <tal:block repeat="widget view/widgets">
      <tr>
        <td class="fieldname">
          <span tal:content="widget/label">LABEL</span>:
        </td>
        <td>
          <span tal:content="structure widget">
            <input type="text" />
          </span>
        </td>
      </tr>
    </tal:block>
  </tbody>
</table>

<table i18n:domain="waeup.kofa" class="table">
  <thead>
    <tr>
      <th i18n:translate="">Id</th>
      <th i18n:translate="">Name</th>
      <th i18n:translate="">Booked Beds</th>
    </tr>
  </thead>
  <tbody>
    <tal:tbody repeat="value context/values">
      <tr tal:define = "stats value/bed_statistics">
        <td> <a tal:attributes="href python: view.url(value)">
            <span tal:content="value/hostel_id">ID</span></a></td>
        <td tal:content="value/hostel_name">NAME</td>
        <td>
          <span tal:replace="stats/booked">10</span> of <span tal:replace="stats/total">100</span>
        </td>
      </tr>
    </tal:tbody>
  </tbody>
</table>