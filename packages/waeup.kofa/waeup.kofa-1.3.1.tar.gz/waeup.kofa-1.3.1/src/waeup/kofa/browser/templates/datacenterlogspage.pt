<div i18n:domain="waeup.kofa" tal:define="files view/files">
  <p i18n:translate="" tal:condition="not: files">
  Currently no log files are available.
  </p>
  <table tal:condition="files" class="table">
    <thead>
      <tr>
        <th>
        </th>
        <th i18n:translate="">File
        </th>
        <th i18n:translate="">Date
        </th>
      </tr>
    </thead>
    <tbody>
      <tr tal:repeat="file files">
        <td>
          <form method="POST" action="@@show">
            <input i18n:translate="" type="submit" class="btn btn-primary"
                   name="show" tal:attributes="value view/show_button" />
            <input type="hidden" name="logname"
                   tal:attributes="value file/name" />
          </form></td>
        <td tal:content="file/name">file.name</td>
        <td tal:content="file/uploaddate">Fri Jan 15 17:44:25 2010</td>
      </tr>
    </tbody>
  </table>
  <form method="POST">
    <input i18n:translate="" type="submit" class="btn btn-default" name="back"
        tal:attributes="value view/back_button"/>
  </form>
</div>