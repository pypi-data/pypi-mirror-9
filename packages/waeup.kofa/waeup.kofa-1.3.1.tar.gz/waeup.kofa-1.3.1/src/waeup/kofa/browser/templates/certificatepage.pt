<table class="form-table">
<thead>
</thead>
<tbody>
  <tal:block repeat="widget view/widgets">
    <tr>
      <td class="fieldname">
        <span tal:content="widget/label">label</span>:
      </td>
      <td tal:on-error="default">
        <div tal:content="structure widget">
          <input type="text" />
        </div>
      </td>
    </tr>
  </tal:block>
</tbody>
</table>

<h3>
  <span tal:content="context/__name__">Code</span>
  <span i18n:domain="waeup.kofa" i18n:translate="">Courses</span>
</h3>
<br />
<table i18n:domain="waeup.kofa" class="kofa-data-table dataTable">
<thead>
  <tr>
    <th i18n:translate="">Level</th>
    <th i18n:translate="">Semester</th>
    <th i18n:translate="">Cert. Course</th>
    <th i18n:translate="">Dep. Course</th>
    <th i18n:translate="">Title</th>
    <th i18n:translate="">Required</th>
  </tr>
</thead>
<tbody>
  <tr tal:repeat="cc context/values">
     <td>
      <span tal:content="cc/level">
	   LEVEL
      </span>
    </td>
    <td tal:content="cc/course/semester">SEMESTER</td>
    <td>
      <a tal:attributes="href python: view.url(cc)"
	     tal:content="cc/__name__">
    	REFERRER CODE
      </a>
    </td>
    <td>
      <a tal:attributes="href python:view.url(cc.course)"
	     tal:content="cc/course/code">
    	COURSE CODE
      </a>
    <td>
      <span tal:content="cc/course/title">
	   COURSE TITLE
      </span>
    </td>
    <td>
      <span tal:content="cc/mandatory">
	   REQUIRED
      </span>
    </td>
  </tr>
</tbody>
</table>

