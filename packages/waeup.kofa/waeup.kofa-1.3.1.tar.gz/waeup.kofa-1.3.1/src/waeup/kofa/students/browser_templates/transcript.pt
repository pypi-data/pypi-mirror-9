<tal:def define="tdata python:context.getTranscriptData()">
<table class="form-table" i18n:domain="waeup.kofa">
  <thead>
  </thead>
  <tbody>
    <tal:block repeat="widget view/widgets">
      <tr>
        <td>
          <span tal:content="widget/label">label</span>:
        </td>
        <td tal:on-error="default">
          <div tal:content="structure widget">
            <input type="text" />
          </div>
        </td>
      </tr>
    </tal:block>
    <tr>
      <td>
        <span i18n:translate="">Cumulative GPA</span>:
      </td>
      <td>
        <div tal:content="python:tdata[1]">GPA</div>
      </td>
    </tr>
  </tbody>
</table>

<h3 i18n:domain="waeup.kofa" i18n:translate="">Subjects/Courses</h3>

<table i18n:domain="waeup.kofa" class="form-table" >
  <thead>
    <tr>
      <th i18n:translate="">Level</th>
      <th i18n:translate="">Session</th>
      <th i18n:translate="">Term</th>
      <th i18n:translate="">Course</th>
      <th i18n:translate="">Title</th>
      <th i18n:translate="">Credits</th>
      <th i18n:translate="">Grade</th>
    </tr>
  </thead>
  <tbody>
    <tal:levels tal:repeat="level python:tdata[0]">
      <tal:semesters tal:repeat="semester_tickets python:(level['tickets_1'],
                                      level['tickets_2'], level['tickets_3'])">
      <tr tal:repeat="ticket semester_tickets">
        <td tal:content="python: view.level_dict[ticket.level]">LEVEL</td>
        <td tal:content="python: view.session_dict[ticket.level_session]">SESSION</td>
        <td tal:content="python: view.semester_dict[ticket.semester]">SEMESTER</td>
        <td tal:content="ticket/code">CODE</td>
        <td tal:content="ticket/title">TITLE</td>
        <td tal:content="ticket/credits">CREDITS</td>
        <td tal:content="ticket/grade|nothing">GRADE</td>
      </tr>
      </tal:semesters>
    </tal:levels>
  </tbody>
</table>
</tal:def>
