<form action="." tal:attributes="action request/URL"
  i18n:domain="waeup.kofa" method="POST" enctype="multipart/form-data">
  <h3 i18n:translate="">Documents</h3>
  <table class="kofa-data-table dataTableManage">
    <thead>
      <tr>
        <th>&nbsp;
        </th>
        <th i18n:translate="">Id</th>
        <th i18n:translate="">Title</th>
        <th i18n:translate="">Type</th>
        <th i18n:translate="">State</th>
      </tr>
    </thead>
    <tbody>
      <tr tal:repeat="doc context/values"> <td>
          <input type="checkbox" name="val_id"
                 tal:attributes="value doc/document_id" /> </td> <td>
          <a tal:attributes="href python: view.url(doc)"
             tal:content="doc/__name__"> ID</a> </td>
          <td tal:content="doc/title">TITLE</td>
          <td tal:content="doc/translated_class_name">CLASSNAME</td>
          <td tal:content="doc/translated_state">STATE</td>
      </tr>
    </tbody>
  </table>
  <div tal:condition="view/availableActions">
    <span tal:repeat="action view/actions" tal:omit-tag="">
      <input tal:replace="structure action/render"/>
    </span>
  </div>
</form>
