<?xml version="1.0"?>
<registry>
    <records interface="collective.geo.settings.interfaces.IGeoFeatureStyle">
        <value key="marker_image">string:${portal_url}/++resource++collective.js.leaflet/images/marker-icon.png</value>
    </records>

    <record interface="Products.ResourceRegistries.interfaces.settings.IResourceRegistriesSettings" field="resourceBundlesForThemes">
        <value purge="false">
            <element key="(default)" purge="false">
                <element>leaflet.minimap</element>
                <element>leaflet.geosearch</element>
                <element>leaflet.geosearch.openstreetmap</element>
            </element>
        </value>
    </record>
</registry>
