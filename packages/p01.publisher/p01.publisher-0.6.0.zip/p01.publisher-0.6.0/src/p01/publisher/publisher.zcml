<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:p01="http://namespaces.zope.org/p01"
    i18n_domain="p01">

  <p01:publisher
      name="BROWSER"
      factory=".request.BrowserFactory"
      methods="GET POST HEAD"
      mimetypes="*"
      priority="50"
      />

  <p01:publisher
      name="JSONRPC"
      factory=".request.JSONRPCFactory"
      methods="POST"
      mimetypes="application/json-rpc"
      priority="50"
      />

</configure>
