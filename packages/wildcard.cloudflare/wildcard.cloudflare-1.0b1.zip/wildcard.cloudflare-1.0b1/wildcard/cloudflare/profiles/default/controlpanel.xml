<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="wildcard.cloudflare"
    purge="False">

    <configlet
        title="CloudFlare"
        action_id="cloudflare_settings"
        appId="wildcard.cloudflare"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@cloudflare-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>

