# -*- coding: utf-8 -*-
import pyhacc
from optparse import OptionParser

parser = OptionParser()
parser.add_option("-c", "--conn", dest="conn", default=None, help="Connection String")
(options, args) = parser.parse_args()

pyhacc.gui_app(options.conn)

