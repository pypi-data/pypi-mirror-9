import sys
import k3lib as riglib

rig = k3lib.K3()

v = rig.verq('M')
if v:
    print "uC version: %s" % (v)
v = rig.verq('D')
if v:
    print "d1 version: %s" % (v)
v = rig.verq('A')
if v:
    print "d2 version: %s" % (v)
v = rig.verq('F')
if v:
    print "FL version: %s" % (v)
v = rig.verq('V')
if v:
    print "dr version: %s" % (v)

rig.close()
