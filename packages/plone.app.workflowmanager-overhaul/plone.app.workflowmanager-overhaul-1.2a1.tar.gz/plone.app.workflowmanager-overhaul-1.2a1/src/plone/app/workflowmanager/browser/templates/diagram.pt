<div class="dialog-box" id="prompt"
     i18n:domain="plone.app.workflowmanager"
     tal:define="url python: view.get_url('@@workflowmanager-view-graph.gif') + '&randomize=' + view.random_number">
  <h2 i18n:translate="">Diagram</h2>

  <p class='discreet'>
    <a tal:attributes="href url" i18n:translate="">View full size</a>
  </p>

  <img tal:attributes="src url" />

</div>