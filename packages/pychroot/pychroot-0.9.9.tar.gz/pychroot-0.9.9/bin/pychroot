#!/usr/bin/env python

import os.path as osp
import sys

sys.path.insert(1, osp.dirname(osp.dirname(osp.abspath(__file__))))


if __name__ == '__main__':
    try:
        from chroot import cli
    except ImportError:
        sys.stderr.write('Cannot import chroot!\n')
        sys.exit(1)

    cli.main()
