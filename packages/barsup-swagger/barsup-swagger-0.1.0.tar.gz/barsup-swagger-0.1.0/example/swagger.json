{
    "swagger": "2.0",
    "info": {
        "version": "0.1.0",
        "title": "Example"
    },
    "paths": {
        "/add/{x}": {
            "get": {
                "tags": [
                    "CalcController"
                ],
                "operationId": "CalcController.add",
                "summary": "Adds two numbers",
                "description": "Adds one number to another",
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "Bla",
                        "examples": {
                            "application/json": {
                                "success": true,
                                "data": 42
                            }
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "x",
                        "in": "path",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "name": "y",
                        "in": "query",
                        "type": "integer"
                    }
                ]
            }
        },
        "/greet/{who}": {
            "get": {
                "tags": [
                    "HelloController"
                ],
                "operationId": "HelloController.greet",
                "summary": "Green someone",
                "description": "Greet someone by name (optional)",
                "responses": {
                    "200": {
                        "description": "Greeting",
                        "examples": {
                            "application/json": {
                                "success": true,
                                "data": "Hello, Anonymous!"
                            }
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "who",
                        "in": "path",
                        "required": true,
                        "type": "string"
                    }
                ]
            }
        }
    }
}
