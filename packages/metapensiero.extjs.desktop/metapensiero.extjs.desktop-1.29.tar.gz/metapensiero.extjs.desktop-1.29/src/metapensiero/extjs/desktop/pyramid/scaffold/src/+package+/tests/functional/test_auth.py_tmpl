# -*- mode: python; coding: utf-8 -*-

from . import AuthenticatedTestCase


class LogoutTest(AuthenticatedTestCase):
    USERNAME = 'admin'
    PASSWORD = 'admin'

    def test_logout(self):
        cookie = self.app.cookies['{{project}}']
        res = self.app.get('/auth/logout')
        self.assertNotEqual(self.app.cookies['{{project}}'], cookie)
