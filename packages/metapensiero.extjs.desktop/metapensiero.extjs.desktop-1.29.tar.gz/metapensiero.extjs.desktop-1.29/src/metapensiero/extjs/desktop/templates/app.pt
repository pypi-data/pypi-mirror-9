<html xmlns="http://www.w3.org/1999/xhtml"
      xml:lang="en-US"
      lang="en-US"
      xmlns:tal="http://xml.zope.org/namespaces/tal">
  <head profile="http://www.w3.org/2005/10/profile">
    <link rel="icon" type="image/png" href="/static/favicon.png" />
    <link tal:condition="debug" rel="stylesheet" type="text/css"
          href="/desktop/extjs/resources/css/ext-all-debug.css" />
    <link tal:condition="debug" rel="stylesheet" type="text/css"
          href="/desktop/css/action.css" />
    <link tal:condition="debug" rel="stylesheet" type="text/css"
          href="/desktop/css/desktop.css" />
    <link tal:condition="debug" rel="stylesheet" type="text/css"
          href="/desktop/css/filterbar.css" />
    <link tal:condition="debug" rel="stylesheet" type="text/css"
          href="/desktop/css/grid.css" />
    <link tal:condition="debug" rel="stylesheet" type="text/css"
          href="/desktop/css/form.css" />
    <link tal:condition="debug" rel="stylesheet" type="text/css"
          href="/desktop/css/upload.css" />

    <link tal:condition="debug" rel="stylesheet" type="text/css"
          href="/static/css/app.css" />

    <link tal:condition="not debug" rel="stylesheet" type="text/css"
          href="/static/all-styles.css" />

    <script type="text/javascript">
      __title__ = "${app_title}";
      __version__ = "${app_version}";
    </script>

    <script type="text/javascript" src="/catalog"></script>

    <script tal:condition="not debug" type="text/javascript"
            src="/static/ext.js">
    </script>

    <script tal:condition="debug" type="text/javascript"
            src="/desktop/extjs/ext-dev.js">
    </script>

    <script tal:condition="not debug" type="text/javascript">
       Ext.Loader.setConfig({ enabled: false });
    </script>

    <script tal:condition="debug" type="text/javascript">
       Ext.Loader.setConfig({ enabled: true });
    </script>

    <script tal:condition="not debug" type="text/javascript"
            src="/static/all-classes.js">
    </script>

    <script type="text/javascript" src="/static/app.js"></script>

    <script type="text/javascript" src="/extjs-l10n"></script>

    <title>${app_title} (${app_version})</title>
  </head>

  <body></body>
</html>
