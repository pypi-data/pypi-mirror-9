/* OVERWRITE FROM bika.lims/bika_listing.css */
div.bika-listing-table-container {
  margin-top: 10px;
  width:100%;
  border-radius: 5px 5px 5px 5px;
  box-shadow: 0pt 1px 2px rgba(0, 0, 0, 0.5);
  display: inline-block;
  overflow: hidden;
  margin-bottom:10px;
}
table.bika-listing-table,
#content table.bika-listing-table {
  font-size: &dtml-fontSmallSize;;
  border-bottom: medium none;
  width: 100%;
  border: 1px solid rgb(220, 220, 220);
  border: medium none;
  border-collapse: collapse;
  margin-bottom:0px;
}
.bika-listing-table thead tr th.fieldRequired {
  background: url("required.png") no-repeat scroll 5px center transparent;
  padding-left:15px;
}
.bika-listing-table thead tr th.minwidth,
.bika-listing-table thead tr td.minwidth  {
  width:0px;
   whitespace:no-wrap;
}
.bika-listing-table tbody tr td {
  padding:3px 3px 0px 3px;
}
.bika-listing-table tbody tr td input[type="text"] {
    width: 97%;
}
.bika-listing-table tbody tr td input[type="text"].numeric {
    width:auto;
}
.bika-listing-table tbody tr td input[readonly], textarea[readonly] {
  border: 1px solid #CCCCCC;
  border-radius: 2px 2px 2px 2px;
  -moz-border-radius: 2px 2px 2px 2px;
  -webkit-border-radius: 2px 2px 2px 2px;
  padding-top:1px;
  padding-bottom:1px;
  background-color:#FFFFFF;
  color:#555555;
}
.bika-listing-table tbody tr td input:not([type='checkbox'],[type='submit'],[type='button']),
.bika-listing-table tbody tr td input:not([type='checkbox'],[type='submit'],[type='button']):focus {
    -moz-appearance: textfield;
    -moz-user-select: text;
    background-color: -moz-field;
    border: 2px inset threedface;
    color: #3c3c3c;
    cursor: text;
    letter-spacing: normal;
    line-height: normal !important;
    padding: 1px 2px 0px 2px;
    text-align: start;
    text-indent: 0;
    text-rendering: optimizelegibility;
    text-shadow: none;
    text-transform: none;
    word-spacing: normal;
    border: 1px solid #7d7d7d;
    border-radius: 2px 2px 2px 2px;
    -moz-border-radius: 2px 2px 2px 2px;
    -webkit-border-radius: 2px 2px 2px 2px;
    font-family:Helvetica,Arial,Verdana;
}
.bika-listing-table tbody tr td input.datepicker_nofuture,
.bika-listing-table tbody tr td input.datepicker {
    width: 65px;
}
.bika-listing-table tbody tr td input.icdcode,
.bika-listing-table tbody tr td input.icdcode {
    width: 50px;
}
.action_button,
.bika-listing-table tfoot tr td input[type="button"],
.bika-listing-table tfoot tr td input[type="submit"] {
  border-radius: &dtml-buttonBorderRadius; !important;
  background: &dtml-buttonSelectedBackgroundColor;
  color: &dtml-buttonSelectedTextColor; !important;
  border:1px solid &dtml-buttonSelectedBorderColor;
  padding:1px 2px 1px 2px;
  cursor:pointer;
}

