#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'python-coveralls==2.5.0','console_scripts','coveralls'
__requires__ = 'python-coveralls==2.5.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('python-coveralls==2.5.0', 'console_scripts', 'coveralls')()
    )
