<?xml version="1.0" encoding="utf-8"?>
<resource>
  <object class="wxFrame" name="frame_Ctrax_batch">
    <title>Batch Processing</title>
    <object class="wxBoxSizer">
      <orient>wxHORIZONTAL</orient>
      <object class="spacer">
        <size>10,-1</size>
      </object>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <object class="spacer">
            <size>-1,10</size>
          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <object class="sizeritem">
                <object class="wxPanel" name="text_list_panel">
                  <size>500,-1</size>
                </object>
                <option>1</option>
                <flag>wxEXPAND</flag>
              </object>
              <orient>wxHORIZONTAL</orient>
              <object class="spacer">
                <size>10,-1</size>
              </object>
              <object class="sizeritem">
                <object class="wxBoxSizer">
                  <object class="sizeritem">
                    <object class="wxButton" name="button_add">
                      <label>Add</label>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <object class="wxButton" name="button_remove">
                      <label>Remove</label>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <object class="wxButton" name="button_execute">
                      <label>Execute</label>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <object class="wxButton" name="button_close">
                      <label>Close</label>
                    </object>
                  </object>
                  <orient>wxVERTICAL</orient>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
            </object>
            <option>1</option>
            <flag>wxEXPAND</flag>
          </object>
          <object class="spacer">
            <size>-1,10</size>
          </object>
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxStaticLine"/>
            <flag>wxALIGN_CENTRE_HORIZONTAL</flag>
            <minsize>400,10</minsize>
          </object>
          <object class="spacer">
            <size>-1,10</size>
          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              
              <object class="sizeritem">
                <object class="wxBoxSizer">
                  <orient>wxVERTICAL</orient>
                  <object class="sizeritem">
                    <object class="wxCheckBox" name="use_settings">
                      <label>Load settings file</label>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <object class="wxCheckBox" name="movies_flipud">
                      <label>Flip movies vertically</label>
                    </object>
                  </object>
                </object>
                <option>1</option>
                <flag>wxEXPAND</flag>
                <minsize>5,-1</minsize>
              </object>
              
              <object class="sizeritem">
                <object class="wxBoxSizer">
                  <orient>wxVERTICAL</orient>
                </object>
                <option>1</option>
                <flag>wxEXPAND</flag>
                <minsize>5,-1</minsize>
              </object>
              <object class="sizeritem">
                <object class="wxBoxSizer">
                  <orient>wxVERTICAL</orient>
                  <object class="sizeritem">
                    <object class="wxCheckBox" name="save_sbfmf">
                      <label>Save compressed SBFMF files</label>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <object class="wxCheckBox" name="save_csv">
                      <label>Save outputs as CSV</label>
                    </object>
                  </object>
                </object>
                <option>1</option>
                <flag>wxEXPAND</flag>
                <minsize>5,-1</minsize>
              </object>
            </object>
            <option>0</option>
          </object>
          <object class="spacer">
            <size>-1,10</size>
          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <size>150,-1</size>
                  <label>Circular Arena:  </label>
                </object>
                <flag>wxALIGN_RIGHT|wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxChoice" name="arena_choice">
                  <content>
                    <item>Auto-detect in each movie</item>
                    <item>Load one settings file for all movies</item>
                    <item>Load existing for each movie</item>
                    <item>Do not use</item>
                  </content>
                  <selection>0</selection>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL|wxALIGN_CENTRE_HORIZONTAL</flag>
              </object>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <size>150,-1</size>
                  <label>Fly Shape Parameters: </label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxChoice" name="shape_choice">
                  <content>
                    <item>Auto-detect in each movie</item>
                    <item>Load one settings file for all movies</item>
                    <item>Load existing for each movie</item>
                  </content>
                  <selection>0</selection>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL|wxALIGN_CENTRE_HORIZONTAL</flag>
              </object>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <size>150,-1</size>
                  <label>Background Model: </label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxChoice" name="bg_model_choice">
                  <content>
                    <item>Auto-detect in each movie</item>
                    <item>Load one settings file for all movies</item>
                    <item>Load existing for each movie</item>
                  </content>
                  <selection>0</selection>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL|wxALIGN_CENTRE_HORIZONTAL</flag>
              </object>
            </object>
          </object>
          <object class="spacer">
            <size>-1,10</size>
          </object>
        </object>
        <option>1</option>
        <flag>wxEXPAND</flag>
      </object>
      <object class="spacer">
        <size>10,-1</size>
      </object>
    </object>
  </object>
</resource>