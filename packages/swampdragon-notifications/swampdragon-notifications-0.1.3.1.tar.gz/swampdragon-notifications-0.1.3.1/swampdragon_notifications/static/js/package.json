{
    "name": "swampdragon-notifications",
    "version": "0.1.0",
    "main": "index.js",
    "description": "SwampDragon notifications",
    "devDependencies": {
        "swampdragon": "*"
    },
    "scripts": {
        "build": "browserify ./index.js -o ./dist/swampdragon-notifications.js -s notifications",
        "build-dist": "browserify ./index.js -o ./dist/swampdragon-notifications.js -s notifications && yuglify ./dist/swampdragon-notifications.js",
        "watch": "watchify ./index.js -o ./dist/swampdragon-notifications.js -s notifications -v"
    }
}