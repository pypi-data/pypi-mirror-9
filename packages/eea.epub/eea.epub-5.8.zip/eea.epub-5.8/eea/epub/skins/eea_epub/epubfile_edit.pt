<html xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    i18n:domain="eea">
    <body>
        <div metal:define-macro="body" tal:define="view context/@@epub_import_view">
            <form method="post" action="./epub_import_view" enctype="multipart/form-data">
                <fieldset>
                    <input type="file" name="epubFile" />
                    <input type="submit" value="Upload epub file" i18n:attributes="value" />
                </fieldset>
            </form>
        </div>
    </body>
</html>
