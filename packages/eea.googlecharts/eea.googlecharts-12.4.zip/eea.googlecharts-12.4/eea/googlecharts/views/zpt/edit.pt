<div id="googlecharts_config" tal:define="ajax_baseurl string:${view/context/absolute_url}"
     i18n:domain="eea">
    <script type="text/javascript" tal:content="string:ajax_baseurl = '${ajax_baseurl}';available_columns = ${view/get_columns};all_rows = ${view/get_allrows}">
    </script>

    <div class="eea-tutorial" style="float:left; display:block; margin-left:0px; margin-top:10px; margin-bottom:20px;font-size:16px;" tutorial="edit chart" message="Configure google charts or"></div>
    <div style="clear:both"></div>

    <input id="addgooglechart"
           type="button"
           value="Add another visualization"
           class="context btn btn-primary"
           i18n:attributes="value"
           style="float:left;" />

    <div style="clear:both"></div>
    <ul id="googlecharts_list"></ul>
</div>
<div style="clear:both"> </div>
