#! /bin/csh -f

############################################################
# Set some variables
############################################################
# sets the varable "package" to autocopasi
set package=pyfscache

# sets the variable "todir" to /home/lkellman/Code/python
# "todir" is where we will distribute the autocopasi
set todir="${HOME}/Code/python"

############################################################
# No need to change things below here
############################################################

svn status

rm -rf ${todir}/${package}.py
cp ${package}.py ${todir}/${package}.py
