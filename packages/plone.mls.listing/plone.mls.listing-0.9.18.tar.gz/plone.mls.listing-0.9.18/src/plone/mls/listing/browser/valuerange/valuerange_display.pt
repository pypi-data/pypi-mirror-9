<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      i18n:domain="plone.mls.listing"
      tal:omit-tag="">
  <span id="" class=""
        tal:attributes="id view/id;
                        class view/klass;
                        style view/style;
                        title view/title;
                        lang view/lang;
                        onclick view/onclick;
                        ondblclick view/ondblclick;
                        onmousedown view/onmousedown;
                        onmouseup view/onmouseup;
                        onmouseover view/onmouseover;
                        onmousemove view/onmousemove;
                        onmouseout view/onmouseout;
                        onkeypress view/onkeypress;
                        onkeydown view/onkeydown;
                        onkeyup view/onkeyup"><tal:block
        tal:repeat="value view/displayValue"><span class="selected-option"
        tal:content="value" /><tal:block
        tal:condition="not:repeat/value/end">, </tal:block></tal:block></span>
</html>
