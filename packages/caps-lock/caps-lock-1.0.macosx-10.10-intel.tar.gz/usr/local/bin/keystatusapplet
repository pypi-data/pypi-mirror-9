#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'caps-lock==1.0','console_scripts','keystatusapplet'
__requires__ = 'caps-lock==1.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('caps-lock==1.0', 'console_scripts', 'keystatusapplet')()
    )
