<div>
	<div class="col">
		<tal:var i18n:domain="ztfy.captcha">
			<img class="captcha hint" alt="Captcha code !" title="Anti-spam code ! Click image to refresh..."
				 data-ams-hint-gravity="n"
				 data-ams-plugins="captcha"
				 data-ams-plugin-captcha-src="/--static--/ztfy.captcha.myams/js/ztfy.captcha.js"
				 data-ams-plugin-captcha-callback="jQuery.ZTFY.captcha.initCaptcha"
				 tal:attributes="id string:${view/id}-img;
								 data-captcha-id view/captcha_id;
								 data-captcha-view-id view/id;"
				 i18n:attributes="alt; title;"
				 onclick="" />
		</tal:var>
	</div>
	<div class="col col-md-5">
		<input type="text" id="" name="" class="" title="" lang="" disabled=""
			   readonly="" alt="" tabindex="" accesskey="" size="" maxlength=""
			   tal:attributes="id view/id;
							   name view/name;
							   class view/klass;
							   style view/style;
							   title view/title;
							   lang view/lang;
							   onclick view/onclick;
							   ondblclick view/ondblclick;
							   onmousedown view/onmousedown;
							   onmouseup view/onmouseup;
							   onmouseover view/onmouseover;
							   onmousemove view/onmousemove;
							   onmouseout view/onmouseout;
							   onkeypress view/onkeypress;
							   onkeydown view/onkeydown;
							   onkeyup view/onkeyup;
							   value view/value;
							   disabled view/disabled;
							   tabindex view/tabindex;
							   onfocus view/onfocus;
							   onblur view/onblur;
							   onchange view/onchange;
							   readonly view/readonly;
							   alt view/alt;
							   accesskey view/accesskey;
							   onselect view/onselect;
							   size view/size;
							   maxlength view/maxlength" />
	</div>
</div>
