<?xml version="1.0"?>

<form string="Sale Channel" col="6">
    <label name="name"/>
    <field name="name"/>
    <label name="code"/>
    <field name="code"/>
    <label name="active"/>
    <field name="active"/>
    <label name="company"/>
    <field name="company"/>
    <label name="address"/>
    <field name="address"/>
    <label name="source"/>
    <field name="source"/>
    <notebook colspan="6">
        <page string="Access" id="access">
            <field name="read_users" colspan="6"/>
            <field name="create_users" colspan="6"/>
        </page>
        <page string="Order Defaults" id="order">
            <group id="inventory_management" colspan="6">
                <label name="warehouse"/>
                <field name="warehouse"/>
            </group>
            <group id="process_workflow" colspan="6">
                <label name="invoice_method"/>
                <field name="invoice_method"/>
                <label name="shipment_method"/>
                <field name="shipment_method"/>
            </group>
            <group id="pricing_and_payment" colspan="6">
                <label name="payment_term"/>
                <field name="payment_term"/>
                <label name="currency"/>
                <field name="currency"/>
                <label name="price_list"/>
                <field name="price_list"/>
            </group>
        </page>
    </notebook>
</form>
