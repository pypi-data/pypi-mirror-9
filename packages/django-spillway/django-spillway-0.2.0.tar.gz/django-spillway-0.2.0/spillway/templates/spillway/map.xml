<Map background-color="transparent" srs="+init=epsg:3857" minimum-version="0.7.2">
  <Style name="YlGnBu">
    <Rule>
      <!--<RasterSymbolizer opacity="0.4">-->
      <RasterSymbolizer>
        <RasterColorizer default-mode="linear" default-color="transparent" epsilon="0.001">
          <!--<stop value="1" color="#ffffcc"/>-->
          <!--<stop value="250" color="#41b6c4"/>-->
          <!--<stop value="500" color="#253494"/>-->
          <stop value="1" color="#ffffd9"/>
          <stop value="2" color="#edf8b1"/>
          <stop value="3" color="#c7e9b4"/>
          <stop value="4" color="#7fcdbb"/>
          <stop value="5" color="#41b6c4"/>
          <stop value="6" color="#1d91c0"/>
          <stop value="7" color="#225ea8"/>
          <stop value="8" color="#253494"/>
          <stop value="9" color="#081d58"/>
        </RasterColorizer>
      </RasterSymbolizer>
    </Rule>
  </Style>

  <Style name="rdpu">
    <Rule>
      <RasterSymbolizer scaling="bilinear">
        <RasterColorizer default-mode="linear" epsilon="0.001">
          <stop value="1" color="#feebe2"/>
          <stop value="250" color="#f768a1"/>
          <stop value="500" color="#7a0177"/>
        </RasterColorizer>
      </RasterSymbolizer>
    </Rule>
  </Style>

  <Style name="redpurple">
    <Rule>
      <RasterSymbolizer opacity="0.6">
        <RasterColorizer default-mode="linear" default-color="transparent" epsilon="0.001">
          <stop value="1" color="#fde0dd"/>
          <stop value="250" color="#fa9fb5"/>
          <stop value="500" color="#c51b8a"/>
        </RasterColorizer>
      </RasterSymbolizer>
    </Rule>
  </Style>

  <Style name="yelloworangered">
    <Rule>
      <RasterSymbolizer opacity="0.6">
        <RasterColorizer default-mode="linear" default-color="transparent" epsilon="0.001">
          <stop value="1" color="#ffeda0"/>
          <stop value="250" color="#feb24c"/>
          <stop value="500" color="#f03b20"/>
        </RasterColorizer>
      </RasterSymbolizer>
    </Rule>
  </Style>

  <Style name="yellowgreen">
    <Rule>
      <RasterSymbolizer opacity="0.8">
        <RasterColorizer default-mode="linear" default-color="transparent" epsilon="0.001">
          <stop value="1" color="#f7fcb9"/>
          <stop value="250" color="#addd8e"/>
          <stop value="500" color="#31a354"/>
        </RasterColorizer>
      </RasterSymbolizer>
    </Rule>
  </Style>

  <!--% for object in object_list %}-->
  <!--<Layer name="{{ object }}" srs="{{ object.srs.proj4 }}" status="on">-->
    <!--<StyleName>{{ style or object.stylename|default:YlGnBu }}</StyleName>-->
    <!--<Datasource>-->
      <!--<Parameter name="file">{{ object.image.path }}</Parameter>-->
      <!--<Parameter name="type">gdal</Parameter>-->
      <!--<Parameter name="band">1</Parameter>-->
    <!--</Datasource>-->
  <!--</Layer>-->
  <!--% endfor %}-->
</Map>
