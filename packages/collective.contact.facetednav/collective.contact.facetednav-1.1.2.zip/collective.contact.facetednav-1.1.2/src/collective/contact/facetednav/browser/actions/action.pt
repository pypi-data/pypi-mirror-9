<html xmlns="http://www.w3.org/1999/xhtml"
  lang="en"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  i18n:domain="collective.contact.facetednav">

   <a tal:attributes="href view/url;
                      class view/klass;
                      id string:contact-action-${view/name}-${context/UID};
                      onclick view/onclick|nothing;
                      ">
     <img i18n:domain="plone"
          tal:attributes="src string:${view/site_url}/${view/icon};
					      alt view/title;
					      title view/title"
          i18n:attributes="alt; title" />
   </a>

</html>