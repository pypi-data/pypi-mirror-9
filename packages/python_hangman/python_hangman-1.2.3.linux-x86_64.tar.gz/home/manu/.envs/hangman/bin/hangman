# EASY-INSTALL-ENTRY-SCRIPT: 'python-hangman==1.2.3','console_scripts','hangman'
__requires__ = 'python-hangman==1.2.3'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('python-hangman==1.2.3', 'console_scripts', 'hangman')()
    )
