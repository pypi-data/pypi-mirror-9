<?xml version="1.0"?>
<object
  name="portal_controlpanel"
  meta_type="Plone Control Panel Tool"
  i18n:domain="appeus.theme"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  purge="False">

   <configlet
     title="AppEus Configuration" i18n:attributes="title"
     action_id="AppEusConfiguration"
     appId="appeus.theme"
     category="Products" condition_expr=""
     icon_expr="string:$portal_url/product_icon.png"
     url_expr="string:${portal_url}/appeus-configuration"
     visible="True">
      <permission>Manage portal</permission>
   </configlet>

</object>