<?xml version="1.0"?>
<data>
    <xpath expr="/form/label[@name='reference']" position="replace">
    </xpath>
    <xpath expr="/form/field[@name='reference']" position="replace">
    </xpath>
    <xpath expr="/form/field[@name='code']" position="replace">
        <group id="production_product_supply_details" col="8" >
            <label name="code" />
            <field name="code" />
            <label name="reference" />
            <field name="reference" />
            <label name="planned_date" />
            <field name="planned_date" />
            <label name="effective_date" />
            <field name="effective_date" />
        </group>
    </xpath>
    <xpath expr="/form/label[@name='code']" position="replace">
    </xpath>
    <xpath expr="/form/label[@name='planned_date']" position="replace">
    </xpath>
    <xpath expr="/form/field[@name='planned_date']" position="replace">
    </xpath>
    <xpath expr="/form/label[@name='effective_date']" position="replace">
    </xpath>
    <xpath expr="/form/field[@name='effective_date']" position="replace">
        <group colspan="8" id="warehouse_location" col="4">
            <label name="warehouse"/>
            <field name="warehouse"/>
            <label name="location"/>
            <field name="location"/>
        </group>
    </xpath>
    <xpath expr="/form/label[@name='product']" position="replace">
    </xpath>
    <xpath expr="/form/field[@name='product']" position="replace">
        <group col="8" id="product_details">
            <label name="product"/>
            <field name="product" colspan="1" />
            <label name="bom"/>
            <field name="bom" colspan="1" />
            <label name="quantity"/>
            <field name="quantity" colspan="1" />
            <label name="uom"/>
            <field name="uom" colspan="1" widget="selection" />
        </group>
    </xpath>
    <xpath expr="/form/label[@name='bom']" position="replace">
    </xpath>
    <xpath expr="/form/field[@name='bom']" position="replace">
    </xpath>
    <xpath expr="/form/label[@name='quantity']" position="replace">
    </xpath>
    <xpath expr="/form/field[@name='quantity']" position="replace">
    </xpath>
    <xpath expr="/form/label[@name='uom']" position="replace">
    </xpath>
    <xpath expr="/form/field[@name='uom']" position="replace">
    </xpath>
    <xpath expr="/form/notebook/page[@id='other']/label[@name='warehouse']" position="replace">
    </xpath>
    <xpath expr="/form/notebook/page[@id='other']/field[@name='warehouse']" position='replace'>
    </xpath>
    <xpath expr="/form/notebook/page[@id='other']/label[@name='location']" position="replace">
    </xpath>
    <xpath expr="/form/notebook/page[@id='other']/field[@name='location']" position='replace'>
    </xpath>    
</data>
