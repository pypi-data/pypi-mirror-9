<li tal:attributes="class python:(view.cssClass or '') + (' active' if view.selected else '')">
	<a tal:attributes="href view/viewURL">
		<i tal:attributes="class view/iconClass"></i>
		<span tal:content="view/title" i18n:translate="" />
		<span class="label" tal:condition="view/label" tal:content="view/label" />
	</a>
</li>
