<div class="span12" i18n:domain="onf.sda"
	 tal:define="values python:list(view.values)">
	<div class="widget-box">
		<div class="widget-title">
			<span class="icon">
				<i class="icon-list" tal:attributes="class view/icon_class | default"></i>
			</span>
			<h5 tal:content="view/legend | default" i18n:translate="">Table contents</h5>
			<div class="actions" tal:content="structure provider:table_actions" />
		</div>
		<div class="widget-content nopadding"
			 tal:condition="values"
			 tal:content="structure view/renderTable" />
		<div class="widget-content"
			 tal:condition="not:values"
			 tal:content="view/empty_message"
			 i18n:translate="">This table is empty.</div>
	</div>
</div>
