  title = {Looney tunes},
  editor = "Daffy D. Duck",
  year = {2001},
}

%% Test having an entry with capitalized fields.
@CAPITALIZED_FIELD_NAMES1{capitalized_field_names1,
    AUTHOR="H. Schmidt",
    TITLE="Quantum-mechanical random-number generator",
    JOURNAL={J. Appl. Phys.},
    VOLUME={41},
    NUMBER={2},
    YEAR={1970},
    PAGES={462--468}
}

%% Exercise some Unicode a little.
@MULTILINGUAL{multilingual,
  author = {Someone},
  title = {ユーザー別サイト
           简体中文
           크로스 플랫폼으로
מדורים מבוקשים
أفضل البحوث
           Σὲ γνωρίζω ἀπὸ
           Десятую Международную
           แผ่นดินฮั่นเสื่อมโทรมแสนสังเวช
           ∮ E⋅da = Q, n → ∞, ∑ f(i) = ∏ g(i)
           français langue étrangère
           mañana olé},
  year = 2222
}
