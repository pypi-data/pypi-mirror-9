<tal:i18n i18n:domain="ztfy.sendit">
	<a class="btn btn-small btn-warning toleft tip-bottom" title="Register new user" i18n:attributes="title"
	   tal:condition="view/canRegisterUser"
	   tal:attributes="href string:javascript:$.ZTFY.dialog.open('@@${view/registration_view_name}')">
		<i class="icon icon-white icon-plus"></i>
	</a>
	<input type="text" autocomplete="off"
		data-ztfy-widget="multiselect"
		data-multiselect-class="user"
		data-multiselect-search-callback="$.ZTFY.json.getQuery"
		data-multiselect-max-results="20"
		data-multiselect-enable-new="false"
		data-multiselect-complex-search="false"
		tal:attributes="id view/id;
						name view/name;
						class view/klass;
						style view/style;
						title view/title;
						value view/value;
						lang view/lang;
						onclick view/onclick;
						ondblclick view/ondblclick;
						onmousedown view/onmousedown;
						onmouseup view/onmouseup;
						onmouseover view/onmouseover;
						onmousemove view/onmousemove;
						onmouseout view/onmouseout;
						onkeypress view/onkeypress;
						onkeydown view/onkeydown;
						onkeyup view/onkeyup;
						disabled view/disabled;
						tabindex view/tabindex;
						onfocus view/onfocus;
						onblur view/onblur;
						onchange view/onchange;
						readonly view/readonly;
						accesskey view/accesskey;
						onselect view/onselect;
						data-multiselect-search-args string:${view/query_name}${view/auth_plugins_value};
						data-multiselect-references view/principals_map;" />
</tal:i18n>