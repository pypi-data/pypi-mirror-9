<div class="span12" i18n:domain="ztfy.sendit">
	<div class="widget-box">
		<div class="widget-title">
			<span class="icon">
				<i class="icon-share"></i>
			</span>
			<h5 i18n:translate="">List of packets you share</h5>
			<span class="progress" tal:define="quota view/quota_pc">
				<span class="mini" i18n:translate="">Quota usage:</span>
				<span class="progress-inner"
					  tal:attributes="style string:background-position-x: -${quota}px;;"></span>
				<span class="mini" tal:content="string:${quota}%" />
			</span>
		</div>
		<div class="widget-content nopadding"
			 tal:condition="view/values"
			 tal:content="structure view/renderTable" />
		<div class="widget-content"
			 tal:condition="not:view/values"
			 i18n:translate="">You don't share any packet.</div>
	</div>
</div>
