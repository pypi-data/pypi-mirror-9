<div class="span12" i18n:domain="ztfy.sendit">
	<div class="widget-box">
		<div class="widget-title">
			<span class="icon">
				<i class="icon-calendar"></i>
			</span>
			<h5 i18n:translate="">History of packets you shared</h5>
		</div>
		<div class="widget-content nopadding"
			 tal:condition="view/values">
			<tal:var content="structure view/renderTable" />
		</div>
		<div class="widget-content"
			 tal:condition="not:view/values"
			 i18n:translate="">You didn't share any packet.</div>
	</div>
	<tal:var define="batch view/renderBatch" condition="batch">
		<div class="pagination alternate centered">
			<ul>
				<tal:var content="structure batch" />
			</ul>
		</div>
	</tal:var>
</div>
