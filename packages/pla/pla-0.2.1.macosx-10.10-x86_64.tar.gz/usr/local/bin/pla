#!/usr/local/opt/python/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'pla==0.2.1','console_scripts','pla'
__requires__ = 'pla==0.2.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('pla==0.2.1', 'console_scripts', 'pla')()
    )
