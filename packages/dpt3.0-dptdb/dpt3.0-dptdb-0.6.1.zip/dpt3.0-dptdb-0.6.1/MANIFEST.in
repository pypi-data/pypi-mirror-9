# Exclude *.py files generated by build from source distribution.
exclude dptdb/dptapi.py
exclude dptdb/version.py
# Exclude files in package_data generated by build from source distribution.
exclude dptdb/_dptapi.pyd
exclude dptdb/licence.txt
# Include dptdb licence in source distribution
include dptdb/LICENCE
# Include contact details in source distribution
include dptdb/CONTACT
# Include the makefile in source distribution
include dptdb/dptMakefile
# Include both setup scripts in Wine and Microsoft Windows generated source
# distributions.
include setup.py
include _wine_setup.py
# include build helpers in source distribution
include dptversion.py