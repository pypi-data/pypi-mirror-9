#!/usr/local/opt/python/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'pyccoon==0.1.2','console_scripts','pyccoon'
__requires__ = 'pyccoon==0.1.2'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('pyccoon==0.1.2', 'console_scripts', 'pyccoon')()
    )
