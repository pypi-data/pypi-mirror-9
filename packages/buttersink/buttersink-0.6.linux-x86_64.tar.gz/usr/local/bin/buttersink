#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'buttersink==0.6','console_scripts','buttersink'
__requires__ = 'buttersink==0.6'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('buttersink==0.6', 'console_scripts', 'buttersink')()
    )
