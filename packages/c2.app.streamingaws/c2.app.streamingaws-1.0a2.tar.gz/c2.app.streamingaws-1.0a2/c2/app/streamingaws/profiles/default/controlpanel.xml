<?xml version="1.0"?>
<object name="portal_controlpanel"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        i18n:domain="c2.app.streamingaws"
        purge="False">

    <configlet
            title="Streaming AWS setting"
            action_id="streaming_aws_settings"
            appId="streaming_aws_settings"
            category="Products"
            condition_expr=""
            url_expr="string:${portal_url}/@@streaming-aws-settings"
            visible="True"
            i18n:attributes="title">
        <permission>Manage portal</permission>
    </configlet>
    <configlet
            title="Manage S3 Videos"
            action_id="manage_s3_video"
            appId="manage_s3_video"
            category="Products"
            condition_expr=""
            url_expr="string:${portal_url}/@@manage-s3-video"
            visible="True"
            i18n:attributes="title">
        <permission>Manage portal</permission>
    </configlet>

</object>