{
    "cwl": "http://github.com/common-workflow-language#",
    "avro": "http://github.com/common-workflow-language#avro/",
    "wfdesc": "http://purl.org/wf4ever/wfdesc#",
    "cnt": "http://www.w3.org/2011/content#",
    "sch": "http://schema.org/",
    "dct": "http://purl.org/dc/terms/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",

    "value": "rdf:value",
    "label": "rdfs:label",
    "description": "dct:description",

    "null": "avro:null",
    "boolean": "avro:boolean",
    "int": "avro:int",
    "long": "avro:long",
    "float": "avro:float",
    "double": "avro:double",
    "bytes": "avro:bytes",
    "string": "avro:string",
    "record": "avro:record",
    "enum": "avro:enum",
    "array": "avro:array",
    "map": "avro:map",
    "File": "cwl:File",

    "id": "@id",
    "ref": "@id",
    "class": "@type",

    "Expression": "cwl:Expression",
    "ExpressionTool": "cwl:ExpressionTool",
    "script":"cwl:expressionProcess",

    "External": "cwl:External",

    "position": "cwl:position",
    "prefix": "cwl:prefix",
    "separator": "cwl:separator",
    "itemSeparator": "cwl:itemSeparator",
    "valueFrom": "cwl:valueFrom",
    "glob": "cwl:glob",

    "name": "avro:name",
    "type": {
        "@id": "avro:type",
        "@type": "@vocab"
    },
    "fields": {
        "@id": "avro:fields",
        "@container": "@list"
    },
    "symbols": {
        "@id": "avro:symbols",
        "@container": "@list"
    },
    "items": {
        "@id": "avro:items",
        "@type": "@vocab"
    },
    "values": {
        "@id": "avro:values",
        "@type": "@vocab"
    },
    "commandLineBinding": "cwl:commandLineBinding",
    "outputBinding": "cwl:outputBinding",
    "default": "cwl:default",

    "CommandLineTool": "cwl:CommandLineTool",
    "baseCommand": {
        "@id": "cwl:baseCommand",
        "@container": "@list"
    },
    "arguments": {
        "@id": "cwl:arguments",
        "@container": "@list"
    },
    "stdin": "cwl:stdin",
    "stdout": "cwl:stdout",
    "inputs": "wfdesc:hasInput",
    "outputs": "wfdesc:hasOutput",
    "requirements": "cwl:requirements",
    "hints": "cwl:hints",
    "schemaDefs": "cwl:schemaDefs",
    "expressionDefs": "cwl:expressionDefs",
    "fileDefs": "cwl:fileDefs",
    "environmentDefs": "cwl:environmentDefs",

    "filename": "cwl:filename",
    "env": "cwl:env",

    "JavascriptExpression": "cwl:JavascriptExpression",
    "invoke": {
        "@id": "cwl:invoke",
        "@container": "@list"
    },

    "DockerRequirement": "cwl:DockerRequirement",
    "dockerPull": "cwl:dockerPull",
    "dockerImport": "cwl:dockerLoad",
    "dockerImageId": "cwl:dockerImageId",

    "CPURequirement": "cwl:CPURequirement",
    "preferredCPUCores": "cwl:preferredCPUCores",

    "MemoryRequirement": "cwl:MemoryRequirement",
    "memoryInMiB": "cwl:memoryInMiB",

    "Workflow": "cwl:Workflow",
    "ExpressionTool": "cwl:ExpressionTool",

    "steps": "wfdesc:hasSubProcess",
    "impl": {
        "@id": "cwl:processImplementation",
        "@type": "@id"
    },
    "def": {
        "@id": "cwl:parameterDefinition",
        "@type": "@id"
    },
    "source": {
        "@id": "wfdesc:hasSource",
        "@type":"@id"
    },
    "connect": {
        "@reverse": "wfdesc:hasSink",
        "@type": "@id"
    }
}
