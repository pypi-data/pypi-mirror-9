{
    "@context": {
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "cwl": "http://github.com/common-workflow-language#",
        "avro": "http://github.com/common-workflow-language#avro/",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "wfdesc": "http://purl.org/wf4ever/wfdesc#",
        "properties": {
            "@reverse": "rdfs:domain"
        }
    },

    "@graph": [
        {
            "@id":"avro:Datatype",
            "@type":"rdfs:Class"
        },
        {
            "@id":"avro:null",
            "@type":"rdfs:Class",
            "rdfs:subClassOf": "avro:Datatype"
        },
        {
            "@id":"avro:boolean",
            "@type":"rdfs:Class",
            "rdfs:subClassOf": "avro:Datatype"
        },
        {
            "@id":"avro:int",
            "@type":"rdfs:Class",
            "rdfs:subClassOf": "avro:Datatype"
        },
        {
            "@id":"avro:long",
            "@type":"rdfs:Class",
            "rdfs:subClassOf": "avro:Datatype"
        },
        {
            "@id":"avro:float",
            "@type":"rdfs:Class",
            "rdfs:subClassOf": "avro:Datatype"
        },
        {
            "@id":"avro:double",
            "@type":"rdfs:Class",
            "rdfs:subClassOf": "avro:Datatype"
        },
        {
            "@id":"avro:bytes",
            "@type":"rdfs:Class",
            "rdfs:subClassOf": "avro:Datatype"
        },
        {
            "@id":"avro:string",
            "@type":"rdfs:Class",
            "rdfs:subClassOf": "avro:Datatype"
        },
        {
            "@id":"avro:record",
            "@type":"rdfs:Class",
            "rdfs:subClassOf": "avro:Datatype"
        },
        {
            "@id":"avro:enum",
            "@type":"rdfs:Class",
            "rdfs:subClassOf": "avro:Datatype"
        },
        {
            "@id":"avro:array",
            "@type":"rdfs:Class",
            "rdfs:subClassOf": "avro:Datatype"
        },
        {
            "@id":"avro:map",
            "@type":"rdfs:Class",
            "rdfs:subClassOf": "avro:Datatype"
        },
        {
            "@id":"cwl:File",
            "@type":"rdfs:Class",
            "rdfs:subClassOf": "avro:Datatype",
            "properties": [
                {
                    "@id": "cwl:path",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:string"
                }
            ]
        },

        {
            "@id": "cwl:Expression",
            "@type": "rdfs:Class",
            "properties": [
                {
                    "@id": "rdfs:value"
                }
            ]
        },

        {
            "@id": "cwl:Binding",
            "@type": "rdfs:Class",
            "properties": [
                {
                    "@id": "cwl:position",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:integer"
                },
                {
                    "@id": "cwl:prefix",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:string"
                },
                {
                    "@id": "cwl:separator",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:string"
                },
                {
                    "@id": "cwl:itemSeparator",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:string"
                },
                {
                    "@id": "cwl:valueFrom",
                    "@type": "rdfs:Property"
                },
                {
                    "@id": "cwl:glob",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:string"
                }
            ]
        },

        {
            "@id": "cwl:Schema",
            "@type": "rdfs:Class",
            "properties": [
                {
                    "@id": "arvo:name",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:string"
                },
                {
                    "@id": "arvo:type",
                    "@type": "rdfs:Property",
                    "rdfs:range": "avro:Datatype"
                },
                {
                    "@id": "arvo:fields",
                    "@type": "rdfs:Property",
                    "rdfs:range": "rdfs:List"
                },
                {
                    "@id": "arvo:symbols",
                    "@type": "rdfs:Property",
                    "rdfs:range": "rdfs:List"
                },
                {
                    "@id": "arvo:items",
                    "@type": "rdfs:Property",
                    "rdfs:range": "avro:Datatype"
                },
                {
                    "@id": "arvo:values",
                    "@type": "rdfs:Property",
                    "rdfs:range": "avro:Datatype"
                },
                {
                    "@id": "cwl:binding",
                    "@type": "rdfs:Property",
                    "rdfs:range": "cwl:Binding"
                }
            ]
        },

        {
            "@id": "cwl:ToolDependency",
            "@type": "rdfs:Class"
        },

        {
            "@id": "cwl:FileDef",
            "@type": "rdfs:Class",
            "properties": [
                {
                    "@id": "cwl:filename",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:string"
                },
                { "@id": "rdf:value"}
            ]
        },

        {
            "@id": "cwl:EnvironmentDef",
            "@type": "rdfs:Class",
            "properties": [
                {
                    "@id": "cwl:env",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:string"
                },
                { "@id": "rdf:value"}
            ]
        },

        {
            "@id": "cwl:CommandLineTool",
            "@type": "rdfs:Class",
            "rdfs:subClassOf": "wfdesc:Process",
            "properties": [
                {
                    "@id": "cwl:baseCommand",
                    "@type": "rdfs:Property",
                    "rdfs:range": ["xsd:string", "rdfs:List"]
                },
                {
                    "@id": "cwl:arguments",
                    "@type": "rdfs:Property",
                    "rdfs:range": "rdfs:List"
                },
                {
                    "@id": "cwl:stdin",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:string"
                },
                {
                    "@id": "cwl:stdout",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:string"
                },
                {
                    "@id": "wfdesc:hasInput",
                    "@type": "rdfs:Property",
                    "rdfs:range": "wfdesc:Input"
                },
                {
                    "@id": "wfdesc:hasOutput",
                    "@type": "rdfs:Property",
                    "rdfs:range": "wfdesc:Output"
                },
                {
                    "@id": "cwl:requirements",
                    "@type": "rdfs:Property",
                    "rdfs:range": "cwl:ToolDependency"
                },
                {
                    "@id": "cwl:hints",
                    "@type": "rdfs:Property",
                    "rdfs:range": "cwl:ToolDependency"
                },
                {
                    "@id": "cwl:schemaDefs",
                    "@type": "rdfs:Property",
                    "rdfs:range": "cwl:Schema"
                },
                {
                    "@id": "cwl:expressionDefs",
                    "@type": "rdfs:Property",
                    "rdfs:range": "rdfs:List"
                },
                {
                    "@id": "cwl:fileDefs",
                    "@type": "rdfs:Property",
                    "rdfs:range": "cwl:FileDef"
                },
                {
                    "@id": "cwl:environmentDefs",
                    "@type": "rdfs:Property",
                    "rdfs:range": "cwl:EnvironmentDef"
                }
            ]
        },

        {
            "@id": "wfdesc:Input",
            "@type": "rdfs:Class",
            "rdfs:subClassOf": "cwl:Schema"
        },

        {
            "@id": "wfdesc:Output",
            "@type": "rdfs:Class",
            "rdfs:subClassOf": "cwl:Schema"
        },

        {
            "@id": "cwl:JavascriptExpression",
            "@type": "rdfs:Class",
            "rdfs:subClassOf": "cwl:Expression",
        },

        {
            "@id": "cwl:DockerRequirement",
            "@type": "cwl:Class",
            "rdfs:subClassOf": "cwl:ToolDependency",
            "properties": [
                {
                    "@id": "cwl:dockerPull",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:string"
                },
                {
                    "@id": "cwl:dockerLoad",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:string"
                },
                {
                    "@id": "cwl:dockerImageId",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:string"
                }
            ]
        },

        {
            "@id": "cwl:CPURequirement",
            "@type": "cwl:Class",
            "rdfs:subClassOf": "cwl:ToolDependency",
            "properties": [
                {
                    "@id": "cwl:preferredCPUCores",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:integer"
                }
            ]
        },

        {
            "@id": "cwl:MemoryRequirement",
            "@type": "cwl:Class",
            "rdfs:subClassOf": "cwl:ToolDependency",
            "properties": [
                {
                    "@id": "cwl:memoryInMiB",
                    "@type": "rdfs:Property",
                    "rdfs:range": "xsd:integer"
                }
            ]
        }

    ]
}
