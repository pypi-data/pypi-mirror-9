<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False">

 <object name="edm_listing_images"
    meta_type="Filesystem Directory View"
    directory="collective.edm.listing:skins/edm_listing_images"/>

 <object name="edm_listing_styles"
    meta_type="Filesystem Directory View"
    directory="collective.edm.listing:skins/edm_listing_styles"/>

 <skin-path name="*">
  <layer name="edm_listing_images" insert-after="custom"/>
 </skin-path>

 <skin-path name="*">
  <layer name="edm_listing_styles" insert-after="custom"/>
 </skin-path>

</object>
