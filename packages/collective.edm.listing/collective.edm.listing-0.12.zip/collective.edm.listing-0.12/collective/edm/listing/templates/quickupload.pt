<dl class="portlet QuickUploadPortlet"
    id="quickupload-viewlet"
    i18n:domain="collective.edm.listing"
    tal:condition="view/available">

    <dt class="quickuploadViewletHeader" i18n:translate="">
        Import files
    </dt>

    <dd>
      <script  type="text/javascript" tal:content="view/javascript">
      </script>
      <div class="uploaderContainer">
          <input type="hidden"
                 name="uploadUrl"
                 class="uploadUrl"
                 tal:attributes="value view/getUploadUrl" />
          <input type="hidden"
                 name="uploadData"
                 class="uploadData"
                 value="" />
      </div>
    </dd>

</dl>
