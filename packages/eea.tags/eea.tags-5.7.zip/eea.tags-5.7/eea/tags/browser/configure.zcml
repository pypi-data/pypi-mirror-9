<configure
  xmlns="http://namespaces.zope.org/zope"
  xmlns:browser="http://namespaces.zope.org/browser"
  i18n_domain="eea">

  <browser:resource
    name="eea.tags.css"
    file="./css/tags.css"
   />

  <browser:resource
    name="eea.tags.js"
    file="./js/tags.js"
    />

  <browser:view
    for="*"
    name="eea.tags.json"
    class=".tags.QueryTags"
    permission="zope2.View"
    />

</configure>
