#!/usr/local/opt/python3/bin/python3.4
# EASY-INSTALL-ENTRY-SCRIPT: 'hooktrigger==0.1.3','console_scripts','hooktrigger'
__requires__ = 'hooktrigger==0.1.3'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('hooktrigger==0.1.3', 'console_scripts', 'hooktrigger')()
    )
