#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'omegaplot==0.5','console_scripts','omegamap'
__requires__ = 'omegaplot==0.5'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('omegaplot==0.5', 'console_scripts', 'omegamap')()
    )
