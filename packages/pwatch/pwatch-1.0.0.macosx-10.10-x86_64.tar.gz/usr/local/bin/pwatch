#!/usr/local/opt/python/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'pwatch==1.0.0','console_scripts','pwatch'
__requires__ = 'pwatch==1.0.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('pwatch==1.0.0', 'console_scripts', 'pwatch')()
    )
