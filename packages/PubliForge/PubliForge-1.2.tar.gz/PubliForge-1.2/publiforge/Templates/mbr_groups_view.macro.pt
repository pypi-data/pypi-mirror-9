<div metal:define-macro="membergroups_view" tal:omit-tag=""
       xmlns:tal="http://xml.zope.org/namespaces/tal"
       xmlns:metal="http://xml.zope.org/namespaces/metal">
  <div class="listActions" tal:condition="member_groups">
    <div class="first">
      <img src="/Static/Images/group22.png" alt="${_('Member groups')}"/>
      <label>${_('Member groups')}</label>
    </div>
    <div class="last">Â </div>
  </div>
  <table class="list smallList" tal:condition="member_groups">
    <caption>${_('Member groups')}</caption>
    <tr>
      <th>${_('Label')}</th>
      <th>${_('Permission')}</th>
    </tr>
    <tr tal:repeat="member_group member_groups">
      <td><a href="${route('group_view', group_id=member_group[0])}">${member_group[1]}</a></td>
      <td>${PERMS[member_group[2]]}</td>
    </tr>
  </table>
</div>




