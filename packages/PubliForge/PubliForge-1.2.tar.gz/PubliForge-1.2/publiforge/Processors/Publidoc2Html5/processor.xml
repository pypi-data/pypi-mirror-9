<publiforge version="1.0">
  <processor>

    <label xml:lang="en">Publidoc XML to HTML 5</label>
    <label xml:lang="fr">XML Publidoc vers HTML 5</label>

    <description xml:lang="en">
      Convert a Publidoc XML document into HTML 5 files with images, sounds
      and videos.
    </description>
    <description xml:lang="fr">
      Convertit un document XML Publidoc en fichiers HTML 5 avec images, sons
      et vidéos.
    </description>

    <module>publiforge.lib.processor.leprisme</module>

    <ancestors>
      <ancestor>Publidoc2XHtml</ancestor>
    </ancestors>

    <variables>
      <group-file>../Publidoc2XHtml/Variables/publidoc2xhtml_module.pfvar.xml</group-file>
      <group-file>../Publidoc2XHtml/Variables/publidoc2xhtml_image.pfvar.xml</group-file>
      <group-file>Variables/publidoc2html5_multimedia.pfvar.xml</group-file>
      <group-file>../Publidoc2XHtml/Variables/publidoc2xhtml_misc.pfvar.xml</group-file>
      <group-file>../Publidoc2XHtml/Variables/publidoc2xhtml.pfvar.xml</group-file>
    </variables>

    <output>Result/Html5</output>

  </processor>
</publiforge>
