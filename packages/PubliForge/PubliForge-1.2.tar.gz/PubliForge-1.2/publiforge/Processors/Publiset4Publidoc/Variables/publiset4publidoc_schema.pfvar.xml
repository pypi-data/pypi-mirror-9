<publiforge version="1.0">
  <variables>
    <group>
      <label xml:lang="en">Schema</label>
      <label xml:lang="fr">Schéma</label>

      <var name="div1_xslt" type="string">
        <default>composition.xsl</default>
        <label xml:lang="en">XSL for main division</label>
        <label xml:lang="fr">XSL pour la division principale</label>
      </var>

      <var name="div1_xpath" type="string">
        <label xml:lang="en">XPath for main division</label>
        <label xml:lang="fr">XPath pour la division principale</label>
      </var>

      <var name="div1_path" type="string">
        <default>../../..</default>
        <label xml:lang="en">Path for main division</label>
        <label xml:lang="fr">Chemin pour la division principale</label>
      </var>

      <var name="div1_head_transform" type="string">
        <label xml:lang="en">Main head transformation</label>
        <label xml:lang="fr">Transformation de l'en-tête principal</label>
      </var>
    </group>
  </variables>
</publiforge>
