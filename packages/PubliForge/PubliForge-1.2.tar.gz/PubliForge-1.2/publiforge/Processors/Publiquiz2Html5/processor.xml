<publiforge version="1.0">
  <processor>

    <label xml:lang="en">Publiquiz XML to HTML 5</label>
    <label xml:lang="fr">XML Publiquiz vers HTML 5</label>

    <description xml:lang="en">
      Convert a Publiquiz XML exercise into HTML 5 files with JavaScript,
      images and sounds.
    </description>
    <description xml:lang="fr">
      Convertit un exercice XML Publiquiz en fichiers HTML 5 avec JavaScript,
      images et sons.
    </description>

    <module>publiforge.lib.processor.leprisme</module>

    <ancestors>
      <ancestor>Publidoc2Html5</ancestor>
    </ancestors>

    <variables>
      <group-file>../Publidoc2XHtml/Variables/publidoc2xhtml_module.pfvar.xml</group-file>
      <group-file>../Publidoc2XHtml/Variables/publidoc2xhtml_image.pfvar.xml</group-file>
      <group-file>../Publidoc2Html5/Variables/publidoc2html5_multimedia.pfvar.xml</group-file>
      <group-file>../Publidoc2XHtml/Variables/publidoc2xhtml_misc.pfvar.xml</group-file>
      <group-file>Variables/publiquiz2html5_quiz.pfvar.xml</group-file>
      <group-file>Variables/publiquiz2html5.pfvar.xml</group-file>
    </variables>

    <output>Result/Html5</output>

  </processor>
</publiforge>
