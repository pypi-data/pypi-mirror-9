<publiforge version="1.0">
  <processor>

    <label xml:lang="en">Pack to Publiset XML file for Publiquiz</label>
    <label xml:lang="fr">Lot vers fichier XML Publiset pour
    Publiquiz</label>

    <description xml:lang="en">
      Transform pack structure into a Publiset XML assembly file for
      Publiquiz.
    </description>
    <description xml:lang="fr">
      Transforme la structure d'un lot en un fichier d'assemblage XML Publiset
      pour Publiquiz.
    </description>

    <module>publiforge.lib.processor.publiseter</module>

    <ancestors>
      <ancestor>Publiset4Publidoc</ancestor>
    </ancestors>

    <variables>
      <group-file>../Publiset4Publidoc/Variables/publiset4publidoc_module.pfvar.xml</group-file>
      <group-file>../Publiset4Publidoc/Variables/publiset4publidoc_meta.pfvar.xml</group-file>
      <group-file>../Publiset4Publidoc/Variables/publiset4publidoc_schema.pfvar.xml</group-file>
      <group-file>../Publidoc2XHtml/Variables/publidoc2xhtml_image.pfvar.xml</group-file>
      <group-file>../Publidoc2XHtml/Variables/publidoc2xhtml_multimedia.pfvar.xml</group-file>
    </variables>

    <output>Result/Xml</output>

  </processor>
</publiforge>
