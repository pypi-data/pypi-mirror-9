<publiforge version="1.0">
  <opener>

    <label xml:lang="en">Publiset XML</label>
    <label xml:lang="fr">XML Publiset</label>

    <description xml:lang="en">
      Display and edit a Publiset XML assembly file.
    </description>
    <description xml:lang="fr">
      Affiche et édite un fichier d'assemblage XML Publiset.
    </description>

    <module>publiforge.lib.opener.xml</module>

    <ancestors>
      <ancestor>Publidoc</ancestor>
    </ancestors>
    
    <imports>
      <import to="RelaxNG/publiset.rng">
        ../../Processors/PublidocValid/RelaxNG/publiset.rng
      </import>
    </imports>
    
    <seeds>
      <seed>
        <file>Seeds/publiset_composition.xml</file>
        <label xml:lang="en">Publiset XML ‒ Composition for Publidoc</label>
        <label xml:lang="fr">XML Publiset ‒ Composition pour Publidoc</label>
      </seed>

      <seed>
        <file>Seeds/publiset_selection.xml</file>
        <label xml:lang="en">Publiset XML ‒ Selection for Publidoc</label>
        <label xml:lang="fr">XML Publiset ‒ Sélection pour Publidoc</label>
      </seed>
    </seeds>

    <variables>
      <group-file>Variables/publiset.pfvar.xml</group-file>
    </variables>

    <public>Static</public>

  </opener>
</publiforge>
